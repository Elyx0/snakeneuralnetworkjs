{
  "presets": [
    ["env", { "targets": { "node": 7, "browsers": ["last 2 versions"] }, "useBuiltIns": true }],
    "stage-0"
  ],
  "env": {
    "production": {
      "plugins": ["babel-plugin-dev-expression"]
    },
    "development": {
      "plugins": [
        "transform-class-properties",
        "transform-es2015-classes",
      ]
    }
  }
}
